 <querysource>
  <name>dati_udc</name>
  <sql>select
 udcdudc as &quot;udc&quot;,
 udnmdis as &quot;distinta&quot;,
 udcdflg as &quot;stato&quot;,
 sum(punmcpe) as &quot;num_copie&quot;,
 max(data_ora(udtmins)) as &quot;data_ora_ins&quot;,
 max(data_ora('now')) as &quot;data_ora_stampa&quot;
from carichi_udc,carichi_prodotti_udc
 where
 udcdudc=pucdudc and
 udcdudc=&lt;? value('udc') ?>
group by udc,distinta,stato;</sql>
 </querysource>
 <querysource>
  <name>lista_articoli_in_udc</name>
  <sql>select
 u.UDCDUDC as &quot;udc&quot;,
 r.PUCDPRO as &quot;codice&quot;,
 r.PUNMCPE as &quot;quantita&quot;,
 c.PRDSTIT as &quot;titolo&quot;
from carichi_udc u,carichi_prodotti_udc r,catalogo c where
 u.UDCDUDC=&lt;? value('udc') ?> and
 r.PUCDUDC=u.UDCDUDC and
 r.PUCDPRO=c.PRCDPRO
order by udc, codice;</sql>
 </querysource>
