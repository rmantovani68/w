{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "main",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/exe/main",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/exe",
        "environment": [],
        "externalConsole": false, //set to true to see output in cmd instead
        "MIMode": "gdb",
        "miDebuggerPath": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
      }
      ]
  }
  