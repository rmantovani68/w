CREATE TABLE public.imballi (
    codice character(18),
    descriz character(40),
    lunghezza integer,
    altezza integer,
    larghezza integer,
    tara integer,
    pesomax integer,
    volperc smallint,
    tplavor smallint,
    volperc1 integer DEFAULT 0,
    volperc2 integer DEFAULT 0
);


ALTER TABLE public.imballi OWNER TO roberto;

--
-- Data for Name: imballi; Type: TABLE DATA; Schema: public; Owner: roberto
--

COPY public.imballi (codice, descriz, lunghezza, altezza, larghezza, tara, pesomax, volperc, tplavor, volperc1, volperc2) FROM stdin;
H0001               SCATOLE IMBALLO CDP 1                       283 180 188 220 999999  78  0   77  78
H0002               SCATOLE IMBALLO CDP 2                       381 160 240 200 999999  78  0   77  77
H0003               SCATOLE IMBALLO CDP 3                       384 326 283 480 999999  76  0   82  81
\.


--
-- Name: imballi_codimb; Type: INDEX; Schema: public; Owner: roberto
--

CREATE INDEX imballi_codimb ON public.imballi USING btree (codice);

